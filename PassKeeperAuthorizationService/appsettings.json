{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",

  "PasswordOptions": {
    "RequiredLength" : 5,
    "RequireNonAlphanumeric" : true,
    "RequireLowercase" : true,
    "RequireUppercase" : true,
    "RequireDigit" : true
  }
}